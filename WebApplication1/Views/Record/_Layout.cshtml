
@{
    ViewData["Title"] = "订单管理";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- 左侧菜单选项========================================= -->
<div class="container-fluid">
    <div class="row-fluie">
        <div class="col-sm-3 col-md-2 sidebar">
            <ul class="nav nav-sidebar">
                <li><a href="/Record/Add" class="nav-header menu-first collapsed" data-toggle="collapse"> <i class="fa fa-plus"></i>&nbsp;增加订单</a></li>
                <li><a href="/Record/Update" class="nav-header menu-first collapsed" data-toggle="collapse"> <i class="fa fa-edit"></i>&nbsp;修改订单</a></li>
                <li><a href="/Record/Delete" class="nav-header menu-first collapsed" data-toggle="collapse"> <i class="fa fa-remove"></i>&nbsp;删除订单</a></li>
